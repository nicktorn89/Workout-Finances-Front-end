  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "./src/**/*{ts,tsx}": [
      "eslint . --ext .js,.jsx,.ts,.tsx --fix"
    ],
    "styled.{ts,tsx}": [
      "stylelint"
    ]
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ]
}
