    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.2.0",
    "jest": "^24.7.1",
    "jest-enzyme": "^7.0.2",
    "lint-staged": "^8.1.0",
    "node-sass": "4.14.1",
    "preload-webpack-plugin": "^3.0.0-beta.3",
    "prettier": "1.15.2",
    "react-addons-test-utils": "^15.6.2",
